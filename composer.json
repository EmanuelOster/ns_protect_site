{
    "name": "nitsan/ns-protect-site",
    "type": "typo3-cms-extension",
    "description": "This extnesion make secure your pages with password",
    "homepage": "https://extensions.typo3.org/extension/ns_protect_site/",
    "license": "GPL-2.0+",
    "keywords": [
        "TYPO3 CMS",
        "Protect",
        "Page",
        "Site",
        "Password",
        "Algorithem",
        "NITSAN"
    ],
    "authors": [
        {
          "name": "NITSAN Technologies",
          "role": "Developer",
          "homepage": "https://www.nitsan.in/"
        }
    ],
    "require": {
        "typo3/cms-core": ">8.0"
    },
    "autoload": {
        "psr-4": {
            "Nitsan\\NsProtectSite\\": "Classes"
        }
    },    
    "replace": {
        "nitsan/ns-protect-site": "self.version",
        "typo3-ter/ns-protect-site": "self.version"
    }
}